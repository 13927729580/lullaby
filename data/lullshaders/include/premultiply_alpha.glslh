#ifndef THIRD_PARTY_LULLABY_DATA_LULLSHADERS_INCLUDE_PREMULTIPLY_ALPHA_GLSLH_
#define THIRD_PARTY_LULLABY_DATA_LULLSHADERS_INCLUDE_PREMULTIPLY_ALPHA_GLSLH_

vec4 PremultiplyAlpha(vec4 c) { return vec4(c.rgb * c.a, c.a); }

#endif  // THIRD_PARTY_LULLABY_DATA_LULLSHADERS_INCLUDE_PREMULTIPLY_ALPHA_GLSLH_
